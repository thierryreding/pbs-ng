description: libraries to handle ELF object files and DWARD debugging information
version: 0.186
license: LGPL-3.0-or-later, GPL-2.0-OR-LATER, GPL-3.0-or-later

depends:
  - devel/glibc
  - libs/curl
  - libs/zlib

files:
  - download:
      url: https://sourceware.org/elfutils/ftp/${version}/elfutils-${version}.tar.bz2
      sha1: 650d52024be684dabf18a5261a69836a16f84f72

packages:
  libelf:
    files:
      - /usr/lib/*.so.*
      - /usr/lib/elfutils

  libelf-dev:
    files:
      - /usr/include
      - /usr/lib/*.a
      - /usr/lib/*.so
      - /usr/lib/pkgconfig

