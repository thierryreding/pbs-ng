[host_machine]
system = '@OS@'
cpu_family = '@ARCH@'
cpu = '@CPU@'
endian = '@ENDIAN@'

[binaries]
c = ['@HOST@-gcc', '--sysroot', '@SYSROOT@']
cpp = ['@HOST@-g++', '--sysroot', '@SYSROOT@']
ar = '@HOST@-ar'
strip = '@HOST@-strip'
pkgconfig = 'pkg-config'
llvm-config = '@HOST@-llvm-config'

[properties]
root = '@SYSROOT@'
