[host_machine]
system = '@OS@'
cpu_family = '@ARCH@'
cpu = '@CPU@'
endian = '@ENDIAN@'

[binaries]
c = '@HOST@-gcc'
cpp = '@HOST@-g++'
ar = '@HOST@-ar'
strip = '@HOST@-strip'
pkgconfig = 'pkg-config'
llvm-config = '@HOST@-llvm-config'

[properties]
root = '@SYSROOT@'
c_args = [@CFLAGS@]
c_link_args = [@LDFLAGS@]
cpp_args = [@CXXFLAGS@]
cpp_link_args = [@LDFLAGS@]
